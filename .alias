unset SSH_AUTH_SOCK
export GIT_EDITOR="vim"
export VIDEX="/opt/videx"
export TERM=xterm-256color
export JAVA_HOME="/usr/lib/jvm/latest"
export GOROOT="/usr/local/go"
export GOPATH="$HOME/gocode"
export GOBIN="$GOPATH/bin"
export GO_LOGGING_CONFIG="$HOME/logging.ini"
export PATH="$JAVA_HOME/bin:/usr/local/bin:$HOME/bin:$GOROOT/bin:$GOBIN:$VIDEX/bin:$PATH"
export EDITOR="/usr/bin/vim"
export C_COLON_BACKSLASH='C:${PWD//\//\\\}>'
alias .="cd .."
alias ..="cd ../.."
alias ...="cd ../../.."
alias ....="cd ../../../.."
alias .....="cd ../../../../.."
alias ......="cd ../../../../../.."
alias .......="cd ../../../../../../.."
alias ........="cd ../../../../../../../.."
alias .........="cd ../../../../../../../../.."
alias ..........="cd ../../../../../../../../../.."
alias ...........="cd ../../../../../../../../../../.."
alias ............="cd ../../../../../../../../../../../.."
alias .............="cd ../../../../../../../../../../../../.."
alias ..............="cd ../../../../../../../../../../../../../.."
alias cdd="source cdd"
alias dos='export PS1=$C_COLON_BACKSLASH'
alias fgrep="find . | awk '!/CVS|\.git|\.svn|_Stub|\.#/  {print \$0;}' | sed 's/\ /\\\ /g' | xargs grep --color=always -in "
alias grepc="grep --color=always -i"
alias less="less -R"
alias locate="locate -i"
alias lpr="lpr -o Duplex=DuplexNoTumble"
alias ls="ls -hl --color"
alias meminfo="ps aux | awk '{print \$4\" \"\$11}' |  uniq -c | awk '{print \$2\" \"\$3}' | sort -nr|head -3"
alias notebook="ipython notebook --pylab=inline --notebook-dir=/home/swilliams/notebooks"
alias sr="screen -R -d"
alias ssh_tunnel="ssh -C2qTnN -D 8080 xeed@xeed.me"
alias tmux="tmux -2 attach || tmux -2"
alias vi="vim"
alias gitpp='git pull && git push'
